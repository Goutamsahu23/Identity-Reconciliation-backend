{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "rootDir": "./src",
      "outDir": "./dist",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*"]
  }
  